@model PropertyMarketplace.Pages.RealEstatePartial1Model
   


            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <div class="form-group">
                    <label asp-for="Property.HouseType" class="control-label"></label>
                    <select asp-for="Property.HouseType" class="form-control" asp-items="@Html.GetEnumSelectList<HouseTypes>()"></select>
                    <span asp-validation-for="Property.HouseType" class="text-danger"></span>
                </div>
                <div class="form-group">
                    <label asp-for="Property.EnergyRating" class="control-label"></label>
                    <input asp-for="Property.EnergyRating" class="form-control" />
                    <span asp-validation-for="Property.EnergyRating" class="text-danger"></span>
                </div>
                <div class="form-group">
                    <label asp-for="Property.NumberOfRooms" class="control-label"></label>
                    <input asp-for="Property.NumberOfRooms" class="form-control" />
                    <span asp-validation-for="Property.NumberOfRooms" class="text-danger"></span>
                </div>
                <div class="form-group">
                    <label asp-for="Property.FloorArea" class="control-label"></label>
                    <input asp-for="Property.FloorArea" class="form-control" />
                    <span asp-validation-for="Property.FloorArea" class="text-danger"></span>
                </div>

                <div class="form-group">
                    <input type="submit" value="Create" class="btn btn-primary" />
                </div>
                
            </div>
        




        @section Scripts {
            @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
        }
